{
    "name": "spomky-labs/oauth2-server-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"OAuth2 Server Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppBundle\\Context\\": "tests/Context"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",

        "spomky-labs/oauth2-server-bundle": "^3.0.0@dev",
        "spomky-labs/oauth2-server-library": "^1.0.0@dev",
        "spomky-labs/jose-bundle": "^1.0.0@dev",
        "spomky-labs/jose": "^1.0.0@alpha",
        "puli/composer-plugin": "^1.0-beta9@dev",
        "puli/symfony-bundle": "^1.0-beta9@dev",
        "puli/twig-extension": "^1.0-beta7",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "bmatzner/foundation-bundle": "~5.3",
        "bmatzner/fontawesome-bundle": "~4.4",
        "bmatzner/jquery-bundle": "*",
        "knplabs/knp-menu-bundle": "~2.0",
        "florianbelhomme/flob-foundation-bundle": "~2.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^2.7.0|^3.0.0",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "vipsoft/doctrine-data-fixtures-extension": "*",
        "behat/behat": "^3.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.2",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "*",
        "puli/repository": "^1.0-beta9@dev",
        "puli/discovery": "^1.0-beta9@dev",
        "puli/url-generator": "^1.0-beta9@dev",
        "puli/cli": "^1.0-beta10@dev",
        "puli/manager": "^1.0-beta10@dev"
      },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "beta",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
          "dev-master": "5.0-dev"
        }
    }
}

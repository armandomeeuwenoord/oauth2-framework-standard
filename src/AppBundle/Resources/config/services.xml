<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.user_manager" class="AppBundle\Entity\UserManager" parent="fos_user.user_manager.default">
        </service>

        <service id="app.client_manager" class="AppBundle\Entity\ClientManager" parent="oauth2_server.client_manager.default">
        </service>

        <service id="app.access_token_manager" class="AppBundle\Entity\AccessTokenManager" parent="oauth2_server.jwt_access_token.manager">
        </service>

        <service id="app.refresh_token_manager" class="AppBundle\Entity\RefreshTokenManager" parent="oauth2_server.refresh_token.manager.default">
        </service>

        <service id="app.auth_code_manager" class="AppBundle\Entity\AuthCodeManager" parent="oauth2_server.auth_code.manager.default">
        </service>

        <service id="app.hash_subject_type" class="OAuth2\OpenIdConnect\Pairwise\HashedSubjectIdentifier">
            <argument>ABCDEFGHIJKLMNOPQRSTUVWXYZ</argument>
            <argument>sha512</argument>
            <argument>abcdefghijklmnopqrstuvwxyz</argument>
        </service>
    </services>
</container>
